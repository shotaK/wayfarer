{
  "name": "Wayfarer",
  "description": "Website keyboard navigation mapper",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content-script.js"],
      "css": ["content-style.css"]
    }
  ]
}
